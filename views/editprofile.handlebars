<div class="dashboard-hero">
    <h1 class="mb-5">Edit Account Information</h1>
    <div class="row">
        <div class="edit-profile-form">
            <form class="row g-3">

                <div class="col-md-6 pt-3">
                    <label for="first_name" class="form-label">First Name:</label>
                    <input type="text" class="form-control" id="edit-first-name" placeholder="{{users.first_name}}">
                </div>

                <div class="col-md-6 pt-3">
                    <label for="last_name" class="form-label">Last Name:</label>
                    <input type="text" class="form-control" id="edit-last-name" placeholder="{{users.last_name}}">
                </div>

                <div class="col-md-6 pt-3">
                    <label for="email" class="form-label">Email:</label>
                    <input type="text" class="form-control" id="edit-email" placeholder="{{users.email}}">
                </div>

                <div class="col-md-6 pt-3">
                    <label for="address" class="form-label">Address:</label>
                    <input type="address" class="form-control" id="edit-address" placeholder="{{users.address}}">
                </div>

                <div class="col-md-6 pt-3">
                    <label for="city" class="form-label">City:</label>
                    <input type="city" class="form-control" id="edit-city" placeholder="{{users.city}}">
                </div>

                <div class="col-md-6 pt-3">
                    <label for="state" class="form-label">State:</label>
                    <input type="state" class="form-control" id="edit-state" placeholder="{{users.state}}">
                </div>

                <div class="col-md-6 pt-3">
                    <label for="zipCode" class="form-label">ZipCode:</label>
                    <input type="zipCode" class="form-control" id="edit-zipCode" placeholder="{{users.zipCode}}">
                </div>

                <div class="col-md-12">
                    <button type="submit" class="btn btn-primary">Save Changes</button>
                </div>

            </form>

        </div>
    </div>
</div>



<script src="/js/edit-profile.js"></script>